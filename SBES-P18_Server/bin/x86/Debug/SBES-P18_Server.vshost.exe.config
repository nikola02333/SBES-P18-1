<?xml version="1.0" encoding="utf-8" ?>
<configuration>
<system.serviceModel>
  <services>
    <service name="SBES_P18_Server.LoadBalancerService">
      <host>
        <baseAddresses>
          <add baseAddress="net.tcp://localhost:27000/ILoadBalancerService" />
        </baseAddresses>
      </host>
      <!-- Service Endpoints -->
      <endpoint address="" binding="netTcpBinding" contract="Common.ILoadBalancerService" />
    </service>


    <!-- Service Endpoints -->
    
     <service name="SBES_P18_Server.WorkerLB">
      <host>
        <baseAddresses>
          <add baseAddress="net.tcp://localhost:29000/IWorkerLB" />
        </baseAddresses>
      </host>
      <!-- Service Endpoints -->
      <endpoint address="" binding="netTcpBinding" contract="WcfService3.IWorkerLB" />
    </service>
    
    
  </services>
</system.serviceModel>
</configuration>